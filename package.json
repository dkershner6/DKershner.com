{
  "name": "DKershner.com",
  "description": "Derek Kershner's website.",
  "version": "1.1.0",
  "author": "Derek Kershner",
  "private": true,
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "dev": "next dev",
    "build": "npm run build:content && npm run build:next",
    "build:next": "next build",
    "build:content": "processmd content/**/*.md --outputDir public/blog --preview 110 --summaryOutput public/blog/summary.json",
    "build:analyze": "cross-env ANALYZE=true npm run build",
    "export": "next export",
    "build-export": "npm run build && npm run export",
    "start": "next start",
    "format": "prettier --write .",
    "format-check": "prettier --check .",
    "test": "jest --coverage",
    "lint": "eslint ./src --ext .tsx --ext .js",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "bootstrap": "^4.5.0",
    "globby": "^11.0.0",
    "lodash.startcase": "^4.4.0",
    "netlify-cms-app": "^2.12.13",
    "netlify-cms-lib-widgets": "^1.3.0",
    "netlify-cms-media-library-cloudinary": "^1.3.4",
    "netlify-cms-media-library-uploadcare": "^0.5.5",
    "next": "^9.4.4",
    "react": "^16.13.1",
    "react-arrow": "^2.0.4",
    "react-bootstrap": "^1.0.1",
    "react-dom": "^16.13.1",
    "react-ga": "^3.0.0",
    "react-icons": "^3.10.0",
    "react-lazy-hero": "^2.0.0",
    "styled-components": "^5.1.1",
    "uuid": "^8.2.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^9.4.4",
    "@testing-library/jest-dom": "^5.9.0",
    "@testing-library/react": "^10.0.4",
    "@types/bootstrap": "^4.5.0",
    "@types/jest": "^25.2.3",
    "@types/node": "^14.0.22",
    "@types/react": "^16.9.35",
    "@types/react-bootstrap": "^1.0.1",
    "@types/react-dom": "^16.9.8",
    "@types/uuid": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "cross-env": "^7.0.2",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "processmd": "^4.5.0",
    "ts-jest": "^26.1.0",
    "typescript": "^3.9.3"
